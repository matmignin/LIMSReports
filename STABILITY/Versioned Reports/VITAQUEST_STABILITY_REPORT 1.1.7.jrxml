	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT
    trq.requestguid,
    Decode(coa_results.formulationid,trq.formulationid,coa_results.Formulationid,trq.formulationid) AS formulationid,
    p.tradename                                                                                     AS tradename,  
    ps.batchnumber                                                                                  As batchnumber,
    UPPER(ps.studyname) 																																																														AS study_name,
    Decode(stb.core_batch,coa_results.coated,'',stb.core_batch)                                         AS stability_core_batch,
    regexp_replace(f.generic1, 'each.*\w*(\(\d*\).*).contain.', '\1', 1, 1,'i')	                    AS serving_size, 
    f.generic4                                                                                      AS fill_weight,
    decode(stb.core_batch, ps.batchnumber,null, coa_results.coated)                                AS coated_lot,
            (
            SELECT DISTINCT
                LISTAGG(generic01, ', ') WITHIN GROUP(ORDER BY batchnumber)
            FROM
                physicalsample
            WHERE
                batchnumber = ps.batchnumber
                                                                                                    )AS package_lot,
            Coa_results.lot                                                                         AS stability_lot,
            CASE
                WHEN ps.StorageCondition Like '25%' OR trq.StorageCondition Like '25%' THEN 
                    '25°C ± 2°C/60% RH ± 5% RH'                
                WHEN ps.StorageCondition Like '40%' OR trq.StorageCondition Like '40%' THEN 
                    '40°C ± 2°C/75% RH ± 5% RH'                    
                WHEN ps.studyname LIKE '%RT%' AND ps.studyname NOT LIKE '%ACC%' AND ps.StorageCondition IS NULL AND trq.StorageCondition IS NULL THEN
                    '25°C ± 2°C/60% RH ± 5% RH'
                WHEN ps.studyname NOT LIKE '%RT%' AND ps.studyname LIKE '%ACC%' AND ps.StorageCondition IS NULL AND trq.StorageCondition IS NULL THEN
                    '40°C ± 2°C/75% RH ± 5% RH'
                WHEN ps.studyname LIKE '%RT%' AND ps.studyname LIKE '%ACC%' AND ps.StorageCondition IS NULL AND trq.StorageCondition IS NULL THEN
                    'Initial'
          
    END                                                                                             AS condition,
    decode(coa_results.formulationid, NULL, stb.customer, coa_results.generic02)                    AS ship_to,
    to_char(to_date(substr(coa_sign.completed_date, 1, 8), 'YYYYMMDD'), 'MM/DD/YYYY')               AS coa_completed_date,
    to_char(to_date(substr(coa_sign.finished_date, 1, 8), 'YYYYMMDD'), 'MM/DD/YYYY')                AS coa_reviewed_date,
    coa_results.manufacture_date,
    coa_results.expiration_date,
    coa_results.carton_lot,
    coa_sign.approvedby                                                                             AS completedby_userid,
    e.employeetitle                                                                                 AS completedby_title,
    e.employeefirstname|| ' ' || e.employeename                                                     AS completedby_name,
    coa_sign.reviewedby                                                                             AS reviewedby_userid,
    e2.employeetitle                                                                                AS reviewedby_title,
    e2.employeefirstname|| ' ' || e2.employeename                                                   AS reviewedby_name,
    (Select listagg(i2.generic2||' '||i2.description, chr(10)) Within GROUP (ORDER BY i2.position)
            FROM Formulation f2
            JOIN Ingredient i2 ON i2.FormulationGUID = f2.FormulationGUID

            where f2.productid = PS.Product
            and i2.Deletion <>'Y'
            AND UPPER(i2.IngredientID) like 'STABILITY NOTE %')                                     AS Stability_Note, --1.0.8
    (
        SELECT
            dv.description
        FROM
            domainvalue dv
        WHERE
                upper(dv.domainid) = 'REPORT PARAMETERS'
            AND upper(dv.value) = 'DATE FORMAT'
            AND dv.deletion = 'N'
    )report_date_format
FROM
         testrequest trq
    JOIN (
        SELECT DISTINCT
            t.requestguid,
            ts.sampleguid
        FROM
                 testsample ts
            JOIN test t ON t.testguid = ts.testguid
                           AND t.deletion = 'N'
        WHERE
            ts.deletion = 'N'
        UNION
        SELECT
            rs.requestguid,
            rs.sampleguid
        FROM
            requestsample rs
        WHERE
            rs.deletion = 'N'
    )              rs ON rs.requestguid = trq.requestguid
    JOIN physicalsample ps ON ps.sampleguid = rs.sampleguid
    LEFT JOIN product p ON p.productid = trq.product
                        AND p.deletion = 'N'
    LEFT JOIN formulation f ON f.productid = p.productid
                        AND f.deletion = 'N'
    LEFT JOIN (
        SELECT
            lwr_id,
            batchname                                                                               AS coated,
            lms_sampleid,
            formulationname                                                                         AS formulation,
            productdesc                                                                             AS customer,
            substr(batchdesc, 10)                                                                   AS batch,
            regexp_substr(batchdesc, '(\d{3}-\d{4})', 1, 1,'i')	             AS Core_batch,
            REGEXP_substr(studydesc, '([0-9/a-z]){5,}',1,1,'i')              AS lot
            ---                 regexp_substr(batchdesc, '(\d{3}-\d{4})', 1, 1,'i')	                                    AS Core_batch,
        FROM
            slim.view_lms_reports@lms_stability_data
        WHERE
            lms_sampleid IS NOT NULL
                                                                                                    )stb 
            ON ps.slimobjectid = stb.lwr_id
            -- AND stb.lms_sampleid = ps.sampleid
             --OR ps.batchnumber = stb.coated
    LEFT JOIN (
        SELECT
            trs.requestguid,
            MAX(trs.circulationid)                                                                  AS max_circulationid,
            t.executeby                                                                             AS approvedby,
            t.finishedby                                                                            AS reviewedby,
            MAX(t.finisheddate)                                                                     AS finished_date,
            MAX(t.completedate)                                                                     AS completed_date
        FROM
                 testrequestsign trs
            JOIN test t ON t.requestguid = trs.requestguid
                           AND t.testgroup = 'Stability'
                           AND t.deletion = 'N'
                           AND t.executeby IS NOT NULL
        GROUP BY
            trs.requestguid,
            t.executeby,
            t.finishedby,
            t.completedate,
            t.finisheddate
    ) coa_sign ON coa_sign.requestguid = trq.requestguid
    LEFT JOIN employee e ON e.empid = coa_sign.approvedby
    LEFT JOIN employee e2 ON e2.empid = coa_sign.reviewedby
    LEFT JOIN (
        SELECT DISTINCT
            ps.batchnumber,
            ps.generic01              															    AS Lot,
            ps.generic02,
            tr3.textresult 																		    AS Carton_Lot,
            ps.generic04,
            ps.generic06                                                                            AS coated,
            trq.product,
            trq.formulationid,
            to_char(to_date(substr(tr1.textresult, 1, 8), 'YYYYMMDD'), 'MM/DD/YYYY')                AS manufacture_date,
            to_char(to_date(substr(tr2.textresult, 1, 8), 'YYYYMMDD'), 
            		decode(tr5.textresult, '', 'MM/DD/YYYY', tr5.textresult)) 					    AS expiration_date,
            tr6.textresult                                                                          AS customer_lot
        FROM
                 testrequest trq
            JOIN requestsample  rs ON rs.requestguid = trq.requestguid
                                     AND rs.deletion = 'N'
            JOIN physicalsample ps ON ps.sampleguid = rs.sampleguid
            JOIN test t ON t.requestguid = trq.requestguid
                               AND t.testgroup = 'COA'
                               AND t.deletion = 'N'
            JOIN testresult tr1 ON tr1.testguid = t.testguid
                               AND tr1.resultid = 'Manufacture Date'
                               AND tr1.resultvaluation = 'OK'
                               AND tr1.deletion = 'N'
            JOIN testresult tr2 ON tr2.testguid = t.testguid
                               AND tr2.resultid = 'Expiration Date'
                               AND tr2.resultvaluation = 'OK'
                               AND tr2.deletion = 'N'
            LEFT JOIN testresult tr3
                                ON tr3.testguid = t.testguid AND
                                tr3.resultid = 'Carton Lot' AND
                                tr3.deletion = 'N'
            LEFT JOIN testresult tr5 ON tr5.testguid = t.testguid
                               AND tr5.resultid = 'Expiration Date Format'
                               AND tr5.resultvaluation = 'OK'
                               AND tr5.deletion = 'N'
            LEFT JOIN testresult tr6 ON tr6.testguid = t.testguid
                               AND tr6.resultid = 'Customer Lot'
                               AND tr6.resultvaluation = 'OK'
                               AND tr6.deletion = 'N'
        WHERE
        substr(trq.batchnumber,1,8) = substr(ps.batchnumber,1,8)
      --  AND trq.product IS NOT NULL
       AND trq.formulationid IS NOT NULL
                                                                                                    ) coa_results 
            ON (substr(coa_results.batchnumber,1,8) = substr(ps.batchnumber,1,8 )
	           OR coa_results.product = trq.product)
            -- AND coa_results.formulationid = trq.formulationid
            AND ( substr(coa_results.batchnumber,1,8) = substr(ps.batchnumber,1,8 ) 
                    OR substr(coa_results.coated,1,8) = substr(ps.batchnumber,1,8 ))
            AND (( ps.generic01 IS NOT NULL 
                    AND coa_results.lot = ps.generic01 ) 
                    OR ps.generic01 IS NULL )
            AND (( ps.generic04 IS NOT NULL
                    AND coa_results.generic04 = ps.generic04 )
                    OR ps.generic04 IS NULL )

WHERE
$X{IN,trq.requestguid,REQUESTGUID}  
order by coa_completed_date desc 
fetch first 1 rows only]]>
	</queryString>
